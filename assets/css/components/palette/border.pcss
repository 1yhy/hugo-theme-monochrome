.border-tp {
  @apply border-transparent dark:border-transparent;
}

.border-200 {
  @apply border-gray-200 dark:border-gray-500;
}

.border-500 {
  @apply border-gray-500 dark:border-gray-300;
}

.border-500-hover {
  @apply hover:border-gray-500 dark:hover:border-gray-300;
}

.border-800 {
  @apply border-gray-800 dark:border-gray-100;
}

.border-800-hover {
  @apply hover:border-gray-800 dark:hover:border-gray-100;
}
